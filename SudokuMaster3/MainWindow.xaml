    <Window.Resources>
        <!-- Having CommandParameter is crucial here -->
        <ContextMenu x:Key="ContextMenu1"
                     Background="White">
            <MenuItem Header="Make 1"
                      Background="White"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 2"
                      Background="White"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 3"
                      Background="White"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 4"
                      Background="White"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 5"
                      Background="White"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 6"
                      Background="White"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 7"
                      Background="White"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 8"
                      Background="White"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <MenuItem Header="Make 9"
                      Background="White"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
            <Separator />
            <MenuItem Header="Erase"
                      Click="Cell_Click"
                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
        </ContextMenu>
        <Style TargetType="Button"
               x:Key="MyButton2">
            <Setter Property="OverridesDefaultStyle"
                    Value="True" />
            <Setter Property="Background"
                    Value="White" />
            <Setter Property="Padding"
                    Value="0" />
            <Setter Property="Margin"
                    Value="0" />
            <Setter Property="Foreground"
                    Value="MediumBlue" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid Background="{TemplateBinding Background}">
                            <Border CornerRadius="0"
                                    BorderBrush="OrangeRed"    
                                    BorderThickness="0">
                                <ContentPresenter x:Name="MyContentPresenter"
                                                  Content="{TemplateBinding Content}"  
                                                  MinHeight="32"
                                                  MinWidth="32"/>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsEnabled"
                         Value="False">
                    <Setter Property="Background"
                            Value="Green" />
                    <Setter Property="Foreground"
                            Value="DeepPink" />
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
